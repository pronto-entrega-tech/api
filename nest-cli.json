{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "typeCheck": true,
    "plugins": ["@nestjs/swagger"],
    "assets": [{ "include": "**/*.html", "outDir": "dist" }]
  },
  "dtoFileNameSuffix": [".dto.ts", ".entity.ts", ".res.ts"]
}
