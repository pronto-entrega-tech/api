{
  "name": "api",
  "author": "ProntoEntrega",
  "private": true,
  "scripts": {
    "use-local-ip": "cross-env USE_LOCAL_IP=true pnpm",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "pnpm start --watch",
    "start:debug": "pnpm start --debug --watch",
    "test": "dotenv -e .env.test -- vitest",
    "test:service": "pnpm test run -t .service-spec.ts$",
    "test:e2e": "pnpm test run --dir test -t .e2e-spec$",
    "test:cov": "pnpm test --coverage",
    "gen-schema": "prisma generate",
    "deploy": "pnpm build && scp -r dist/* api-deployer@192.168.15.13:deploy-cache/dist && ssh api-deployer@192.168.15.13 bash ./deploy.sh",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:dev": "dotenv -e .env.test -- prisma migrate dev",
    "migrate:dev-deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "migrate:prod": "prisma migrate deploy",
    "postinstall": "node shim.js",
    "postuninstall": "pnpm postinstall",
    "pre-commit": "cross-env-shell CI=true \"pnpm gen-schema && pnpm lint && pnpm type-check && pnpm test && pnpm test:e2e\""
  },
  "dependencies": {
    "@bull-board/api": "^4.6.0",
    "@bull-board/fastify": "^4.6.0",
    "@fastify/cookie": "^7.4.0",
    "@fastify/csrf-protection": "^5.1.0",
    "@fastify/helmet": "^9.1.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@metinseylan/nestjs-opentelemetry": "^3.0.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.5",
    "@nestjs/platform-socket.io": "^9.1.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/websockets": "^9.1.5",
    "@opentelemetry/exporter-zipkin": "1.7.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-trace-base": "1.7.0",
