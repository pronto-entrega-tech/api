{
  "name": "api",
  "author": "ProntoEntrega",
  "private": true,
  "scripts": {
    "dotenv": "dotenvx run --convention=nextjs",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "build": "rimraf dist && nest build",
    "start": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "pnpm -s dotenv -- prisma migrate dev",
    "db:reset": "pnpm -s dotenv -- prisma migrate reset",
    "db:studio": "pnpm -s dotenv -- prisma studio",
    "db:deploy": "prisma migrate deploy",
    "test": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest run",
    "test:unit": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest --project unit",
    "test:e2e": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest run --project e2e",
    "pre-commit": "pnpm -s dotenv -e CI=true -- sh -c \"pnpm lint && pnpm type-check && pnpm test\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@amplication/opentelemetry-nestjs": "^5.0.3",
    "@bull-board/api": "^5.21.7",
    "@bull-board/fastify": "^5.21.7",
    "@fastify/cookie": "^9.4.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@nest-lab/fastify-multer": "^1.2.0",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1 ",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/platform-socket.io": "^ 10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.1",
    "@opentelemetry/exporter-zipkin": "1.21.0",
    "@opentelemetry/sdk-trace-node": "1.21.0",
    "@prisma/client": "^5.19.1",
    "argon2": "^0.41.1",
    "axios": "1.7.7",
    "bull": "^4.16.3",
    "bullmq": "^5.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^4.0.0",
    "fastify": "^4.28.1",
    "google-auth-library": "^9.0.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.8.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis-semaphore": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@fastify/static": "^7.0.4",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.26",
    "@types/crypto-js": "^4.1.1",
    "@types/http-proxy": "^1.17.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prisma": "^5.19.1",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}
