{
  "name": "api",
  "author": "ProntoEntrega",
  "type": "module",
  "private": true,
  "scripts": {
    "dotenv": "dotenvx run --convention=nextjs",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "build": "rimraf dist && nest build",
    "start": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "pnpm -s dotenv -- sh -c \"prisma generate && prisma db execute --file prisma/schema.sql && tsx prisma/seed.ts\"",
    "db:studio": "pnpm -s dotenv -- prisma studio",
    "db:deploy": "prisma migrate deploy",
    "test": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest run",
    "test:unit": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest --project unit",
    "test:e2e": "cross-env NODE_ENV=test pnpm -s dotenv -- vitest run --project e2e",
    "pre-commit": "pnpm -s dotenv -e CI=true -- sh -c \"pnpm lint && pnpm type-check && pnpm test\""
  },
  "dependencies": {
    "@bull-board/api": "^4.6.0",
    "@bull-board/fastify": "^4.6.0",
    "@fastify/cookie": "^7.4.0",
    "@fastify/csrf-protection": "^5.1.0",
    "@fastify/helmet": "^9.1.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@metinseylan/nestjs-opentelemetry": "^3.0.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.5",
    "@nestjs/platform-socket.io": "^9.1.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/websockets": "^9.1.5",
    "@opentelemetry/exporter-zipkin": "1.7.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-trace-base": "1.7.0",
    "@opentelemetry/semantic-conventions": "1.7.0",
    "@prisma/client": "^4.5.0",
    "argon2": "^0.30.3",
    "axios": "0.27.2",
    "bull": "^4.10.1",
    "bullmq": "^4.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "fastify": "4.9.2",
    "fastify-file-interceptor": "^1.0.8",
    "glob": "^8.0.3",
    "google-auth-library": "^9.0.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis-semaphore": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.5.7",
    "sharp": "^0.30.7",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@fastify/static": "^6.5.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.5",
    "@swc/core": "^1.7.26",
    "@types/bull": "^3.15.9",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^8.0.0",
    "@types/http-proxy": "^1.17.9",
    "@types/multer": "^1.4.7",
    "@types/node": "16.11.68",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prisma": "^4.5.0",
    "ts-loader": "^9.4.4",
    "tsconfig-paths": "^4.2.0",
