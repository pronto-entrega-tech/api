{
  "name": "api",
  "author": "ProntoEntrega",
  "private": true,
  "scripts": {
    "version": "node -v",
    "postinstall": "node shim.js",
    "postuninstall": "yarn postinstall",
    "gen": "nest g resource",
    "upgrade": "yarn upgrade --latest && syncyarnlock --save --keepPrefix && node shim.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "use-local-ip": "cross-env USE_LOCAL_IP=true",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "dotenv -e .env.test -- vitest",
    "test:dev": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:module": "yarn test -t .module-spec.ts$",
    "test:e2e": "yarn test --dir test -t .e2e-spec$",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "gen-schema": "prisma generate",
    "migrate:dev": "dotenv -e .env.test -- npx prisma migrate dev",
    "migrate:dev-deploy": "dotenv -e .env.test -- npx prisma migrate deploy",
    "migrate:prod": "npx prisma migrate deploy",
    "deploy": "yarn build && scp -r dist/* api-deployer@192.168.15.13:deploy-cache/dist && ssh api-deployer@192.168.15.13 bash ./deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn gen-schema && yarn lint && yarn type-check && yarn test"
    }
  },
  "dependencies": {
    "@bull-board/api": "^4.2.1",
    "@bull-board/fastify": "^4.2.1",
    "@fastify/cookie": "^7.3.1",
    "@fastify/csrf-protection": "^5.1.0",
    "@fastify/helmet": "^9.1.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@metinseylan/nestjs-opentelemetry": "^2.0.7",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.11",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/websockets": "^9.0.11",
    "@opentelemetry/exporter-zipkin": "^1.5.0",
    "@prisma/client": "^4.3.1",
    "argon2": "^0.28.7",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.29.3",
    "fastify-file-interceptor": "^1.0.8",
    "google-auth-library": "^8.1.1",
    "ioredis": "^5.2.2",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis-semaphore": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@fastify/static": "^6.5.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@swc/core": "^1.2.223",
    "@swc/jest": "^0.2.22",
    "@types/bull": "^3.15.9",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^28.1.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.23.4"
  },
  "version": "0.0.0"
}
