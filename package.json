    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^8.0.0",
    "@types/http-proxy": "^1.17.9",
    "@types/multer": "^1.4.7",
    "@types/node": "16.11.68",
