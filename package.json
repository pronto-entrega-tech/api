    "@prisma/client": "^4.5.0",
    "argon2": "^0.30.3",
    "axios": "0.27.2",
    "bull": "^4.10.1",
    "bullmq": "^4.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "fastify": "4.9.2",
    "fastify-file-interceptor": "^1.0.8",
    "glob": "^8.0.3",
    "google-auth-library": "^9.0.0",
