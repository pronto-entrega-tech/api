{
  "name": "api",
  "author": "ProntoEntrega",
  "private": true,
  "scripts": {
    "use-local-ip": "cross-env USE_LOCAL_IP=true",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "test": "dotenv -e .env.test -- vitest",
    "test:service": "yarn test run -t .service-spec.ts$",
    "test:e2e": "yarn test run --dir test -t .e2e-spec$",
    "test:cov": "yarn test --coverage",
    "gen-schema": "prisma generate",
    "update": "yarn upgrade && syncyarnlock --save --keepPrefix && node shim.js",
    "deploy": "yarn build && scp -r dist/* api-deployer@192.168.15.13:deploy-cache/dist && ssh api-deployer@192.168.15.13 bash ./deploy.sh",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:dev": "dotenv -e .env.test -- npx prisma migrate dev",
    "migrate:dev-deploy": "dotenv -e .env.test -- npx prisma migrate deploy",
    "migrate:prod": "npx prisma migrate deploy",
    "postinstall": "node shim.js",
    "postuninstall": "yarn postinstall",
    "prepare": "husky install",
    "pre-commit": "cross-env-shell CI=true \"yarn gen-schema && yarn lint && yarn type-check && yarn test && yarn test:e2e\""
  },
  "dependencies": {
    "@bull-board/api": "^4.6.0",
    "@bull-board/fastify": "^4.6.0",
    "@fastify/cookie": "^7.4.0",
    "@fastify/csrf-protection": "^5.1.0",
    "@fastify/helmet": "^9.1.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@metinseylan/nestjs-opentelemetry": "^2.0.7",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.5",
    "@nestjs/platform-socket.io": "^9.1.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/websockets": "^9.1.5",
    "@opentelemetry/exporter-zipkin": "^1.7.0",
    "@prisma/client": "^4.5.0",
    "argon2": "^0.28.7",
    "bull": "^4.10.1",
    "bullmq": "^3.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.29.3",
    "fastify-file-interceptor": "^1.0.8",
    "glob": "^8.0.3",
    "google-auth-library": "^8.6.0",
    "ioredis": "^5.2.3",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis-semaphore": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "sharp": "^0.30.7"
